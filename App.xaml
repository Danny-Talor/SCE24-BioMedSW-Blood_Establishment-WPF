<Application x:Class="SCE24_BioMedSW_Blood_Establishment_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SCE24_BioMedSW_Blood_Establishment_WPF"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <x:Array x:Key="UserRoles" Type="{x:Type sys:String}">
            <sys:String>Research Student</sys:String>
            <sys:String>Staff Member</sys:String>
            <sys:String>Administrator</sys:String>
        </x:Array>
    </Application.Resources>
</Application>
